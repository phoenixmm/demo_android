<?xml version="1.0" encoding="utf-8"?>
<com.cjf.testdemo.viewpage.ScrollBackView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/gestureview"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A0888800" >

    <com.cjf.testdemo.viewpage.ImageViewPager
        android:id="@+id/viewpage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#A0880088">

    </com.cjf.testdemo.viewpage.ImageViewPager>

    <TextView
        android:id="@+id/number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:background="#A0008888"
        android:text="2/5"/>

</com.cjf.testdemo.viewpage.ScrollBackView>
